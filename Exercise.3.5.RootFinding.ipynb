    "def root(f,a,b):\n",
    "    if f(a)*f(b) > 0:\n",
    "        print(\"function evals have same sign\")\n",
    "        return None\n",
    "    c = (a+b)/2\n",
    "    while(abs(f(c))>1e-8):\n",
    "        if f(a)*f(c)>0:\n",
    "            a=c\n",
    "        b=c\n",
    "    return c"
